      label: '1'
    - name: '2'
      label: '2'
    - name: '3'
      label: '3'
    - name: '4'
      label: '4'
    - name: '5'
      label: '5'
- name: small_aerospike-server_ns_default_ttl
  type: string
  configurable: true
  default: '0S'
  constraints:
    - must_match_regex: '[0-9]+[SMHD]'
