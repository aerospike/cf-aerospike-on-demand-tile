---
name: aerospike-service-on-demand
product_version: '<%= PRODUCT_VERSION %>'
minimum_version_for_upgrade: 0.0.0.0
label: Aerospike EE OnDemand Service
rank: 50
metadata_version: '1.8'
serial: true
service_broker: true # Imp to generate uaa client creds
icon_image: 

releases:
- name: on-demand-service-broker
  file: on-demand-service-broker-<%= SERVICE_BROKER_VERSION %>.tgz
  version: '<%= SERVICE_BROKER_VERSION %>'
- name: aerospike-service-adapter-release
  file: aerospike-service-adapter-release-<%= SERVICE_ADAPTER_VERSION %>.tgz
  version: latest 
- name: aerospike-service-release
  file: aerospike-service-release-<%= SERVICE_RELEASE_VERSION %>.tgz
  version: latest

stemcell_criteria:
  os: ubuntu-trusty
  version: '3263.15'

post_deploy_errands:
- name: register_on_demand_service_broker

pre_delete_errands:
- name: delete_subdeployments_on_demand_service_broker
- name: unregister_on_demand_service_broker

property_blueprints:
- name: encryption_key
  type: secret
- name: broker_basic_auth
  type: simple_credentials

- name: small_az_multi_select
  type: service_network_az_multi_select
  configurable: true
  optional: false
- name: small_aerospike-server_vm_type
  type: vm_type_dropdown
  optional: true
  configurable: true
- name: small_aerospike-server_disk_type
  type: disk_type_dropdown
  configurable: true
  optional: true
- name: small_aerospike-server_instance_count
  type: integer
  configurable: true
  default: 2

- name: small_aerospike-server_ns_name
  type: string
  configurable: true
  default: as_datastore
- name: small_aerospike-server_ns_storage_type
  type: dropdown_select
  configurable: true
  default: 'memory'
  options:
    - name: 'memory'
      label: Memory
    - name: 'device'
      label: Device (Persistent)
- name: small_aerospike-server_ns_memory_size
  type: string
  configurable: true
  default: '2G'
  constraints:
